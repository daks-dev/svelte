{
  "name": "@daks.dev/svelte",
  "version": "0.1.0",
  "description": "DAKS.DEV Svelte monorepo",
  "private": false,
  "scripts": {
    "check": "pnpm -r check",
    "run:prettier": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss",
    "lint": "pnpm run:prettier --check . && eslint .",
    "format": "pnpm run:prettier --write .",
    "package:recursive": "pnpm -r package --filter=./packages/*",
    "up:recursive": "pnpm -r up"
  },
  "devDependencies": {
    "@daks.dev/svelte.sdk": "workspace:^",
    "@stylistic/eslint-plugin-js": "^4.1.0",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/eslint-config": "^8.1.0",
    "@sveltejs/kit": "^1.27.6",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-svelte": "^3.0.2",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "prettier": "^3.5.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^4.2.19",
    "svelte-check": "^3.6.7",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^4.5.9"
  },
  "type": "module",
  "engines": {
    "node": ">=16.20.2",
    "pnpm": ">=8.7.0"
  },
  "browserslist": [
    "last 2 version",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daks-dev/svelte.git"
  },
  "bugs": "https://github.com/daks-dev/svelte/issues",
  "author": {
    "name": "DVA",
    "email": "mail@daks.dev",
    "url": "https://daks.dev"
  },
  "homepage": "https://daks.dev/",
  "keywords": [
    "svelte",
    "sveltekit",
    "tailwindcss",
    "imagetools",
    "vite-imagetools",
    "vite-plugin",
    "yandex"
  ]
}
